{
  "apps": [
    {
      "name": "producer",
      "script": "./start.js",
      "cwd": "/var/www/Producer-oCode",
      "exec_mode": "fork",
      "instances": 1,
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "500M",
      "env": {
        "NODE_ENV": "production"
      },
      "error_file": "/tmp/producer-error.log",
      "out_file": "/tmp/producer-out.log",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "merge_logs": true
    }
  ]
}
